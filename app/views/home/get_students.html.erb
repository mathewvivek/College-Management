  <h3>Student List</h3>

  <table border='1'>
    <thead>
      <tr>
        <th>Name</th>
        <th>Age</th>
        <th>Phone</th>
        <th>Address</th>
        <th>Email</th>
        <th>Identity Proof</th>
        <th>Joined Date</th>
      </tr>
    </thead>

    <tbody>
      <% @students.each do |student| %>
        <tr>
          <td><%= student.name %></td>
          <td><%= student.age %></td>
          <td><%= student.phone %></td>
          <td><%= student.address %></td>
          <td><%= student.email %></td>
          <td><%= student.identity_proof %></td>
          <td><%= student.created_at.to_date %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
